( DEFINE ( DOMAIN OILSPILL )
( :REQUIREMENTS :EQUALITY :NEGATIVE-PRECONDITIONS :CONDITIONAL-EFFECTS :TYPING :UNIVERSAL-PRECONDITIONS )
( :TYPES
	VEHICLE - AGENT
	TASK - OBJECT
	CAPABILITY - OBJECT
	AGENT - OBJECT
)
( :CONSTANTS
)
( :PREDICATES
	( FREE-BLOCK )
	( SELECTING )
	( APPLYING )
	( RESETTING )
	( FREE-AGENT ?AGENT0 - AGENT )
	( BUSY-AGENT ?AGENT0 - AGENT )
	( DONE-AGENT ?AGENT0 - AGENT )
	( HAS ?VEHICLE0 - VEHICLE ?CAPABILITY1 - CAPABILITY )
	( NEEDS ?TASK0 - TASK ?CAPABILITY1 - CAPABILITY )
	( AT ?VEHICLE0 - VEHICLE ?TASK1 - TASK )
	( IDLE ?VEHICLE0 - VEHICLE )
	( BE_DONE_BEFORE ?TASK0 - TASK ?TASK1 - TASK )
	( DONE ?TASK0 - TASK )
	( ACTIVE-DO-TASK ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?CAPABILITY2 - CAPABILITY )
	( REQ-NEG-DO-TASK ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?CAPABILITY2 - CAPABILITY )
	( ACTIVE-MOVE-TO-TASK ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?TASK2 - TASK )
	( REQ-NEG-MOVE-TO-TASK ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?TASK2 - TASK )
)
( :ACTION START
  :PARAMETERS ( )
  :PRECONDITION
	( AND
		( FREE-BLOCK )
	)
  :EFFECT
	( AND
		( NOT ( FREE-BLOCK ) )
		( SELECTING )
	)
)
( :ACTION APPLY
  :PARAMETERS ( )
  :PRECONDITION
	( AND
		( SELECTING )
	)
  :EFFECT
	( AND
		( NOT ( SELECTING ) )
		( APPLYING )
	)
)
( :ACTION RESET
  :PARAMETERS ( )
  :PRECONDITION
	( AND
		( APPLYING )
	)
  :EFFECT
	( AND
		( NOT ( APPLYING ) )
		( RESETTING )
	)
)
( :ACTION FINISH
  :PARAMETERS ( )
  :PRECONDITION
	( AND
		( RESETTING )
		( FORALL
			( ?AGENT0 - AGENT )
			( FREE-AGENT ?AGENT0 )
		)
	)
  :EFFECT
	( AND
		( NOT ( RESETTING ) )
		( FREE-BLOCK )
	)
)
( :ACTION SELECT-DO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?CAPABILITY2 - CAPABILITY )
  :PRECONDITION
	( AND
		( SELECTING )
		( FREE-AGENT ?VEHICLE0 )
		( NOT ( REQ-NEG-DO-TASK ?VEHICLE0 ?TASK1 ?CAPABILITY2 ) )
		( AT ?VEHICLE0 ?TASK1 )
		( HAS ?VEHICLE0 ?CAPABILITY2 )
		( NEEDS ?TASK1 ?CAPABILITY2 )
		( FORALL
			( ?TASK3 - TASK )
			( AND
				( BE_DONE_BEFORE ?TASK3 ?TASK1 )
			)
		)
		( FORALL
			( ?TASK3 - TASK )
			( AND
				( DONE ?TASK3 )
			)
		)
	)
  :EFFECT
	( AND
		( NOT ( FREE-AGENT ?VEHICLE0 ) )
		( BUSY-AGENT ?VEHICLE0 )
		( ACTIVE-DO-TASK ?VEHICLE0 ?TASK1 ?CAPABILITY2 )
	)
)
( :ACTION DO-DO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?CAPABILITY2 - CAPABILITY )
  :PRECONDITION
	( AND
		( APPLYING )
		( BUSY-AGENT ?VEHICLE0 )
		( ACTIVE-DO-TASK ?VEHICLE0 ?TASK1 ?CAPABILITY2 )
	)
  :EFFECT
	( AND
		( NOT ( BUSY-AGENT ?VEHICLE0 ) )
		( DONE-AGENT ?VEHICLE0 )
		( DONE ?TASK1 )
		( IDLE ?VEHICLE0 )
	)
)
( :ACTION END-DO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?CAPABILITY2 - CAPABILITY )
  :PRECONDITION
	( AND
		( RESETTING )
		( DONE-AGENT ?VEHICLE0 )
		( ACTIVE-DO-TASK ?VEHICLE0 ?TASK1 ?CAPABILITY2 )
	)
  :EFFECT
	( AND
		( NOT ( DONE-AGENT ?VEHICLE0 ) )
		( FREE-AGENT ?VEHICLE0 )
		( NOT ( ACTIVE-DO-TASK ?VEHICLE0 ?TASK1 ?CAPABILITY2 ) )
	)
)
( :ACTION SELECT-MOVE-TO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?TASK2 - TASK )
  :PRECONDITION
	( AND
		( SELECTING )
		( FREE-AGENT ?VEHICLE0 )
		( NOT ( REQ-NEG-MOVE-TO-TASK ?VEHICLE0 ?TASK1 ?TASK2 ) )
		( AT ?VEHICLE0 ?TASK1 )
		( IDLE ?VEHICLE0 )
	)
  :EFFECT
	( AND
		( NOT ( FREE-AGENT ?VEHICLE0 ) )
		( BUSY-AGENT ?VEHICLE0 )
		( ACTIVE-MOVE-TO-TASK ?VEHICLE0 ?TASK1 ?TASK2 )
	)
)
( :ACTION DO-MOVE-TO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?TASK2 - TASK )
  :PRECONDITION
	( AND
		( APPLYING )
		( BUSY-AGENT ?VEHICLE0 )
		( ACTIVE-MOVE-TO-TASK ?VEHICLE0 ?TASK1 ?TASK2 )
	)
  :EFFECT
	( AND
		( NOT ( BUSY-AGENT ?VEHICLE0 ) )
		( DONE-AGENT ?VEHICLE0 )
		( NOT ( AT ?VEHICLE0 ?TASK1 ) )
		( AT ?VEHICLE0 ?TASK2 )
	)
)
( :ACTION END-MOVE-TO-TASK
  :PARAMETERS ( ?VEHICLE0 - VEHICLE ?TASK1 - TASK ?TASK2 - TASK )
  :PRECONDITION
	( AND
		( RESETTING )
		( DONE-AGENT ?VEHICLE0 )
		( ACTIVE-MOVE-TO-TASK ?VEHICLE0 ?TASK1 ?TASK2 )
	)
  :EFFECT
	( AND
		( NOT ( DONE-AGENT ?VEHICLE0 ) )
		( FREE-AGENT ?VEHICLE0 )
		( NOT ( ACTIVE-MOVE-TO-TASK ?VEHICLE0 ?TASK1 ?TASK2 ) )
	)
)
)
